{
    "name": "botman/studio",
    "description": "BotMan Chatbot framework.",
    "keywords": [
        "botman",
        "chatbot",
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0",
        "botman/botman": "~2.4",
        "botman/driver-facebook": "^1.6",
        "botman/driver-web": "~1.0",
        "botman/studio-addons": "~1.0",
        "botman/tinker": "~1.0",
        "clue/stdio-react": "^1.0",
        "fideloper/proxy": "~3.3",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "5.5.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maatwebsite/excel": "^3.0",
        "mpociot/slack-client": "^0.3.0",
        "predis/predis": "^1.1",
        "revolution/laravel-google-sheets": "^3.2",
        "spatie/laravel-backup": "^5.5",
        "spatie/laravel-cors": "^1.0",
        "spatie/laravel-image-optimizer": "^1.1",
        "toin0u/geocoder-laravel": "^4.0",
        "yangqi/htmldom": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "filp/whoops": "~2.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Ktbots\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "BotMan\\Studio\\Providers\\DriverServiceProvider::publishDriverConfigurations",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "BotMan\\Studio\\Providers\\DriverServiceProvider::publishDriverConfigurations",
            "php artisan optimize"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    }
}
